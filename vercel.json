{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api?path=$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/[[...path]].js": {
      "excludeFiles": "{public/downloads/**,agent/**,electron/**}"
    }
  }
}
