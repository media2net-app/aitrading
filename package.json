{
  "name": "aitrading",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:agent": "cd agent && npm install && npx pkg . --output ../public/downloads/AITradingAgent",
    "build:agent-electron": "cd electron && npm install && npm run build",
    "postinstall": "prisma generate",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node server.js",
    "server:dev": "npm run build && node server.js",
    "start:all": "concurrently -n backend,frontend -c blue,green \"node server.js\" \"vite --port 3000\""
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/nodemailer": "^7.0.9",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "framer-motion": "^12.33.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.563.0",
    "nodemailer": "^8.0.0",
    "prisma": "^6.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.4",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
}
